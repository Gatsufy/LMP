package eserciziario;

import java.util.ArrayList;

public class Classe {
	
	private String nome;
	
	private int annoNascita;
	
	private int corso;
	
	private char c;
	
	private ArrayList<Studente> listaStudenti;

	public Classe(String nome, int annoNascita, int corso, char c, ArrayList<Studente> listaStudenti) {
		super();
		this.nome = nome;
		this.annoNascita = annoNascita;
		this.corso = corso;
		this.c = c;
		this.listaStudenti=listaStudenti;
	}

	public String getNome() {
		return nome;
	}

	public int getAnnoNascita() {
		return annoNascita;
	}

	public int getCorso() {
		return corso;
	}

	public char getC() {
		return c;
	}

	public ArrayList<Studente> getListaStudenti() {
		return listaStudenti;
	}
	
	

	
	

}
