package eserciziario;

public class PrimoEmicorso {
	
	private ProvaScrittaModuloUno provaScritta;
	
	private ProvaOraleModuloUno provaOrale;
	
	private int coefficienteScritto;
	
	private int coefficienteDue;
	
	

	public PrimoEmicorso(ProvaScrittaModuloUno provaScritta, ProvaOraleModuloUno provaOrale) {
		super();
		this.provaScritta = provaScritta;
		this.provaOrale = provaOrale;
	}

	public ProvaScrittaModuloUno getProvaScritta() {
		return provaScritta;
	}

	public ProvaOraleModuloUno getProvaOrale() {
		return provaOrale;
	}
	
	public int getCoefficiente(){
		return coefficienteUno;
		}
		
	}
	
	
	
	

}
